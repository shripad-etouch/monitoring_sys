<h1>Edit URL</h1>

<form id="edit-url_request_response" name="url_request_response">

  <div class="field">
    <label for="url"> URL:</label>
    <input type="text" name="url" id="url" value="<%= url %>" >
  </div>

  <div class="field">
    <label for="verb_id"> Verb:</label>
    <input type="text" name="verb_id" id="verb_id" value="<%= verb_id %>">
  </div>

  <div class="field">
    <label for="timeout"> Timeout:</label>
    <input type="text" name="timeout" id="timeout" value="<%= timeout %>">
  </div>

  <div class="field">
    <label for="request_object_type_id"> Object:</label>
    <input type="text" name="request_object_type_id" id="request_object_type_id" value="<%= request_object_type_id %>">
  </div>

  <div class="actions">
    <input type="submit" value="Update URL" />
  </div>

</form>

<a href="#/index">Back</a>