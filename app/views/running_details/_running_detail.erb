<table id="schedule-running-details">
  <thead>
    <tr>
     <th>URL</th>
     <th>Verb</th>
     <th>Request Type</th>
     <th>Scheduled On</th>
     <th>Expected Status</th>
     <th>Timeout</th>     
     <th>Start Time</th>
     <th>End Time</th>
     <th>Actual Status</th>
  	</tr>
  </thead>
<% schedule_details.each do |schedule_detail|%>
	<tr>
	 <td><%= schedule_detail.url_request_response.url %></td>
	 <td><%= schedule_detail.url_request_response.http_verb %></td>
	 <td><%= schedule_detail.url_request_response.object_type %></td>
	 <td><%= schedule_detail.url_request_response.time_to_run.to_s(:time_only)%></td>
	 <td><%#= schedule_detail.success_status %></td> 
	 <td><%= schedule_detail.url_request_response.timeout %></td>
	 <td><%= schedule_detail.start_time.to_s(:full_friendly) %></td>
	 <td><%= schedule_detail.end_time.to_s(:full_friendly) %></td>
	 <td><%= schedule_detail.status_result %></td>
	</tr>
<% end %>
</table>
<%= will_paginate @schedule_details %>